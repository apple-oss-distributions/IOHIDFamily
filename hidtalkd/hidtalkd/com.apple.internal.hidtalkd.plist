<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/hidtalkd</string>
	</array>
	<key>Label</key>
	<string>com.apple.internal.hidtalkd</string>
	<key>MachServices</key>
	<dict>
		<key>com.apple.HIDClientRemote</key>
		<true/>
		<key>com.apple.HIDDeviceRemote</key>
		<true/>
	</dict>
	<key>POSIXSpawnType</key>
	<string>Interactive</string>
</dict>
</plist>
